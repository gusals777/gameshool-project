<!-- 모든 그리드 시작 -->  
  
  <!-- 롤 랭킹 그리드 -->
  <div class="content-section-a">
    <div class="container">
      <div class="row">
        
        <div class="col-lg-4 col-md-4 col-sm-6">
          <h4 class="section-heading">리그 오브 레전드</h4>
          <img class="img-responsive" src="http://www.op.gg/r/outImage/https://cdn.leagueoflegends.com/match-history/opengraph_icon.jpg" alt="">
        </div>
        
        <div class="col-lg-2 col-md-2 col-sm-6">
          <hr class="section-heading-spacer">
          <div class="clearfix"></div>
          <h4 class="section-heading">LOL 승점</h4>
            <% @rank_lol_winner_point.each do |t| %>
              <h4><%= t.lol_team_name %></h4>
              <p>승점 : <%= t.lol_winner_point %> 점</p>
            <% end %> 
          <a href="/freematch/total_ranking" onclick="window.open(this.href,'window','width=500,height=600,resizable,scrollbars,toolbar,menubar') ;return false;" class="btn btn-default btn-lg" id="rank_lol_winner_point">전체랭킹</a>
        </div>
        
        <div class="col-lg-2 col-md-2 col-sm-6">
          <hr class="section-heading-spacer">
          <div class="clearfix"></div>
          <h4 class="section-heading">LOL 총경기수</h4>
            <% @rank_lol_total_game.each do |t| %>
              <h4><%= t.lol_team_name %></h4>
              <p>총경기수 : <%= t.lol_total_game %> 점</p>
            <% end %> 
          <a href="/freematch/total_ranking" onclick="window.open(this.href,'window','width=500,height=600,resizable,scrollbars,toolbar,menubar') ;return false;" class="btn btn-default btn-lg" id="rank_lol_total_game">전체랭킹</a>
        </div>
        
        <div class="col-lg-2 col-md-2 col-sm-6">
          <hr class="section-heading-spacer">
          <div class="clearfix"></div>
          <h4 class="section-heading">LOL 승률</h4>
            <% @rank_lol_winning_rate.each do |t| %>
              <h4><%= t.lol_team_name %></h4>
              <p>승률 : <%= t.lol_winning_rate %> %</p>
            <% end %> 
          <a href="/freematch/total_ranking" onclick="window.open(this.href,'window','width=500,height=600,resizable,scrollbars,toolbar,menubar') ;return false;" class="btn btn-default btn-lg">전체랭킹</a>
        </div>
        
        <div class="col-lg-2 col-md-2 col-sm-6">
          <hr class="section-heading-spacer">
          <div class="clearfix"></div>
          <h4 class="section-heading">LOL 트롤</h4>
            <% @rank_lol_lose.each do |t| %>
              <h4><%= t.lol_winning_rate %></h4>
              <p>패 : <%= t.lol_lose %> </p>
            <% end %> 
          <a href="/freematch/total_ranking" onclick="window.open(this.href,'window','width=500,height=600,resizable,scrollbars,toolbar,menubar') ;return false;" class="btn btn-default btn-lg">전체랭킹</a>
        </div>
        
      </div>
    </div>  
  </div>
  
